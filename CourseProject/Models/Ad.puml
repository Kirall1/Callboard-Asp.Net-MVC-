@startuml
class Ad {
    + AdID : long? <<get>> <<set>>
    + Owner : string? <<get>> <<set>>
    + OwnerPhone : string? <<get>> <<set>>
    + City : string? <<get>> <<set>>
    + Name : string? <<get>> <<set>>
    + Description : string? <<get>> <<set>>
    + Price : decimal? <<get>> <<set>>
    + Category : string? <<get>> <<set>>
    + Photos : string? <<get>> <<set>>
    + CreatedDate : DateTime? <<get>> <<set>>
}
@enduml
